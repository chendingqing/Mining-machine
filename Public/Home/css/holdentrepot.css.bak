body{
	font-size: 10px;
	font-family: "微软雅黑";
	margin: 0;
	padding: 0;
	color: #444;
	background: #EFEFF4;
	overflow-y: auto;
}
div,p,h1,h2,h3,h4,h5,h6{
	margin: 0;
	padding: 0;
}
ul,li,dl,dt,dd{
	margin: 0;
	padding: 0;
	list-style: none;
}
a{
	color: #fff;
	text-decoration: none;
}
a:hover{
	color: #fff;
	text-decoration: none;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.fc{
	clear: both;
}
.myimg{
	width: 100%;
}
/* top */
#top{
	width: 100%;
	height: 30px;
	background: #0067AC;
}
#top ul{
    width:100%;
}
#top ul li{
    float:left;
	width:33.3%;
	text-align:center;
	color:#fff;
	line-height:30px;
	font-size:16px;
	font-family:"微软雅黑";
	border-left:1px solid #fff;
	margin-left:-1px;
}
#top ul li.active{
    background:rgba(0,0,0,.7);
	color:#fff;
    transition: all 1s;
}
#top ul li>a{
    display:block;
	width:100%;
	height:100%;
}
#notice{
	width: 100%;
	text-align: center;
	line-height: 30px;
	border-bottom: 1px solid #ddd;
}
/* content */
#content{
	width: 100%;
	padding-bottom: 75px;
}
#content>li{
	background: #fff;
	margin-bottom: 10px;
	width: 100%;
	height: auto;
    overflow: hidden;
    box-shadow: 1px 1px 1px 1px #444;
}
#content .top{
    margin-left: 10px;
    padding-top: 10px;
	height: 70px;
}
#content .top .hn{
    font-size: 24px;
    color: #444;
}
#content .top .hn i{
	font-style:normal;
	display: inline-block;
	width: 24px;
	height: 24px;
	font-size: 16px;
	background: #4CAF50;
	color: #fff;
	text-align: center;
	line-height: 24px; 
	border-radius: 2px;
	position: relative;
    top: -2px;
}
#content .countDown{
	padding: 2px 10px;
	background: #0067AC;
	color: #fff;
	margin-right:10px;
	border-radius: 5px;
}
#content .top .hx{
	margin-top: 10px;
	font-size: 14px;
	margin-left: 34px;
	color: #0067AC;
}
#content .top .fr{
	margin-right: 10px;
	font-size: 20px;
	font-weight: 600;
	text-align: center;
	line-height: 30px;
}
#content .content{
   overflow: hidden;
   width: 100%;
   border-top: 1px solid #ddd;
   border-bottom: 1px solid #ddd;
   height: 63px;
}
#content .content>div{
	width: 33.3%;
	text-align: center;
	font-size: 16px;
	padding-top: 10px;
	padding-bottom: 10px;
	position: relative;
}
#content .content>div>i{
	position: absolute;
	right: 0;
	width: 1px;
	height: 63px;
	top: 0;
	background: #ddd;
}
#content .price1{
	color: #0067AC;
}
#content .price3{
	color: #0067AC;
}
#content .price2{
	color: red;
}
#content .bottom{
	line-height: 30px;
}
#content .bottom .fl{
	margin-left: 5px;
}
#content .bottom .fr{
	margin-right: 5px;
}
