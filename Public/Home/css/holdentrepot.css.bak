/* footer */
#footer{
	height: 50px;
	width: 100%;
	background: #131224;
	position: fixed;
	bottom: 0;
}
#footer ul>li{
   width: 25%;
   text-align:  center;
   height: 50px;
   color: #999;
}
#footer ul>li>a{
	display: block;
    line-height: 20px;
    width: 100%;
    color: #999;
}
#footer ul>li>p{
	margin-top: 10px;
}
#footer ul> .active{
	color: #0067AC;
}
#footer ul> .active>a{
	color: #0067AC;
}
/* 加载提示 */
#loading{
	display: none;
    position: fixed;
    top: 50%;
    left: 50%;
    margin-left: -75px;
    margin-top: -73.5px;
    text-align: center;
    font-size: 16px;
}
#loading .load-container{
	width: 150px;
	height: 30px;
	margin: 0 auto;
}
#loading .load7 .loader{
	font-size: 14px;
	margin: 0 auto;
}
#loading .load7 a{
	position: relative;
	top: 45px;
	padding: 5px 7px;
    background: #0067AC;
    border-radius: 5px;
    font-size: 20px;
    color: #fff;
}